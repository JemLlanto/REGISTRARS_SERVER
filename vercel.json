"installCommand": "npm install --legacy-peer-deps",
"version": 2,
"builds":[
    {
        "src": "index.js",
        "use": "@vercel/node"
    },
    {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "public"
        }
    }

],
"routes": [
    {
        "src": "/(.*)",
        "dest": "/index.js"
    },
    {
        "src": "/(.*)",
        "dest": "/public/$1"
    }
],